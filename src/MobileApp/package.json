{
  "name": "@app/mobile-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "tsc": "tsc",
    "test": "jest --passWithNoTests",
    "test:coverage": "jest --passWithNoTests --coverage --colors && make-coverage-badge --output-path \"./coverage/coverageBadge.svg\" --report-path \"./coverage/coverage-summary.json\"",
    "lint": "eslint ./src --ext .ts,.tsx",
    "lint:fix": "tsc && eslint ./src --ext .ts,.tsx --quiet --fix",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-native": "0.62.2",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@types/jest": "^25.2.1",
    "@types/react-native": "^0.62.0",
    "@types/react-redux": "^7.1.5",
    "@types/react-test-renderer": "16.9.2",
    "babel-jest": "^25.3.0",
    "jest": "^25.3.0",
    "make-coverage-badge": "^1.2.0",
    "metro-react-native-babel-preset": "^0.58.0",
    "react-test-renderer": "16.11.0",
    "redux-saga-test-plan": "^4.0.0-rc.3",
    "typescript": "^3.9.5"
  }
}
